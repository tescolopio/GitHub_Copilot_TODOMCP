{
  "name": "mcp-autonomous-dev",
  "version": "1.0.0",
  "description": "MCP Autonomous Development System - AI-powered autonomous code development workflows",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "docker:build": "docker build -t mcp-todo-management .",
    "docker:run": "docker run -d -p 3000:3000 --name mcp-todo-server mcp-todo-management",
    "docker:stop": "docker stop mcp-todo-server && docker rm mcp-todo-server",
    "package": "npm run build && npm pack",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "autonomous",
    "development",
    "ai",
    "automation",
    "vscode"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/uuid": "^10.0.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "semver": "^7.5.4",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.0"
  }
}
